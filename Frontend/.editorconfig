# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# 4 space indentation
[*]
end_of_line = cr
insert_final_newline = true
indent_style = space
indent_size = 4

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,scss,jsx,css}]
charset = utf-8
